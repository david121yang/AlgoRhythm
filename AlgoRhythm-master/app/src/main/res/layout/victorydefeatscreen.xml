<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_background">

    <ImageView
        android:id="@+id/tracklines2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/StartPoint"
        app:srcCompat="@drawable/tracklines" />

    <ImageView
        android:id="@+id/background"
        android:layout_width="347dp"
        android:layout_height="601dp"
        android:layout_marginStart="32dp"
        android:background="@color/transparentLightBlue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.661"
        app:srcCompat="@color/transparentBlue"
        tools:srcCompat="@color/transparentBlue" />

    <Button
        android:id="@+id/playAgainButton"
        android:layout_width="211dp"
        android:layout_height="47dp"
        android:layout_marginTop="556dp"
        android:layout_marginBottom="32dp"
        android:background="@color/brightBlue"
        android:fontFamily="@font/orbitron"
        android:text="Play Again"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toTopOf="@+id/songListButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/songListButton"
        android:layout_width="202dp"
        android:layout_height="49dp"
        android:layout_marginBottom="64dp"
        android:background="@color/brightBlue"
        android:fontFamily="@font/orbitron"
        android:text="Song List"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_completeOrFailed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/orbitron"
        android:text="Song Complete"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_on_primary"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/background" />

    <TextView
        android:id="@+id/statNames"
        android:layout_width="0dp"
        android:layout_height="24dp"
        android:layout_marginTop="79dp"
        android:layout_marginBottom="75dp"
        android:fontFamily="@font/orbitron"
        android:text="Song:"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/playAgainButton"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/tv_completeOrFailed" />

    <TextView
        android:id="@+id/tv_songName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/orbitron"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBaseline_toBaselineOf="@+id/statNames"
        app:layout_constraintEnd_toEndOf="@+id/tv_score" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="86dp"
        android:layout_marginBottom="87dp"
        android:fontFamily="@font/orbitron"
        android:text="Score:"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/tv_completeOrFailed" />

    <TextView
        android:id="@+id/tv_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/orbitron"
        android:text="score"
        android:textAlignment="textEnd"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/tv_maxCombo" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="19dp"
        android:fontFamily="@font/orbitron"
        android:text="Longest Combo"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/tv_maxCombo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/orbitron"
        android:text="textview"
        android:textAlignment="textEnd"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/tv_notesHit" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/orbitron"
        android:text="Notes Hit:"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.411" />

    <TextView
        android:id="@+id/tv_notesHit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:fontFamily="@font/orbitron"
        android:text="notes hit"
        android:textAlignment="textEnd"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/background" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/orbitron"
        android:text="Ranking"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toTopOf="@+id/tv_rank"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_rank"
        android:layout_width="73dp"
        android:layout_height="62dp"
        android:layout_marginTop="175dp"
        android:layout_marginBottom="176dp"
        android:fontFamily="@font/orbitron"
        android:text="F"
        android:textAlignment="center"
        android:textColor="@color/design_default_color_on_primary"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/songListButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statNames"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>